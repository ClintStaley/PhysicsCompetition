# Cmp server URL and authentication
# Dev setups may need a port #, e.g. example.com:8443
# Example: http://www.softwareinventions.com:8080
# Example: http://localhost:8080
EVC.path=<URL>

# Accept any supplied cert.
EVC.relaxedHTTPS=true

# EVC account name and login
# Example: admin@softwareinventions.com
EVC.user=<adminLogin>
EVC.pass=<adminPass>

# Number of EVC evaluation threads to maintain. 
threads=1

